{
   "providerId":"aweber.com",
   "providerName":"AWeber",
   "serviceId":"email_auth",
   "serviceName":"AWeber Email Authentication",
   "version": 1,
   "logoUrl":"https://assets.aweber-static.com/page-templates/assets/img/powered_by.png",
   "description":"Configure AWeber's SPF and DKIM records to improve sender reputation and deliverability. Connect AWeber landing pages with your domain.",
   "variableDescription":"",
   "records":[
      {
         "type": "SPFM",
         "host": "@",
         "spfRules": "v=spf1 include:send.aweber.com ~all"
      },
      {
         "type": "CNAME",
         "host":"aweberkeya.domainkey.%domain%",
         "pointsTo": "aweberkeya.send.aweber.com",
         "ttl": 3600
       },
       {
         "type": "CNAME",
         "host": "aweberkeyb.domainkey.%domain%",
         "pointsTo": "aweberkeyb.send.aweber.com",
         "ttl": 3600
       },
       {
         "type": "CNAME",
         "host": "aweberkeyc.domainkey.%domain%",
         "pointsTo": "aweberkeyc.send.aweber.com",
         "ttl": 3600
       }
   ]
}